<div class="col-md-2">
  <div class="card">
    <div class="card-header">
      <h5 class="card-title">Moon Phase</h5>
    </div>
    <div class="card-body">
      <ul class="list-group list-group-flush">
        <% @moon_phase_data[:days].first.each do |key, value| %>
          <li class="list-group-item"><%= key %>: <%= value %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
